    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.3.2",
    "svelte": "^5.2.7",
    "svelte-check": "^4.1.0",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.5",
    "sass": "^1.81.0"
